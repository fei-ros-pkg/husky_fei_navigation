<launch>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <remap from="/cmd_vel" to="/husky/cmd_vel" />
    <rosparam file="$(find husky_navigation)/config/local_nav_params/params_costmap2d.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find husky_navigation)/config/local_nav_params/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find husky_navigation)/config/local_nav_params/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find husky_navigation)/config/local_nav_params/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find husky_navigation)/config/base_local_planner_params.yaml" command="load" />
  </node>
</launch>
